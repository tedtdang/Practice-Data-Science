   "source": [
    "def balanced_num(number):\n",
    "    n = str(number)\n",
    "    half = (len(n) - 1) // 2\n",
    "    if len(n) < 3 or sum(map(int, n[:half])) == sum(map(int, n[-half:])):\n",
    "        return 'Balanced'\n",
    "    return 'Not Balanced'"
   ]
